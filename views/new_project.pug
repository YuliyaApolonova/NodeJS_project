extends layout

block content
    include header
    .panel
        .container
            .row
                .col-xs-12.col-md-6.pull-left
                    h1.page-title Add new project
    section
        .container
            ul#category-list.works-nav
                li.works-nav-item(data-category='all')
                    span.nav-link.active(data-category='all') All
                li.works-nav-item(data-category='print')
                    span.nav-link(data-category='print') Print
                li.works-nav-item(data-category='photography')
                    span.nav-link(data-category='photography') Photography
                li.works-nav-item(data-category='web')
                    span.nav-link(data-category='web') Web
                li.works-nav-item(data-category='applications')
                    span.nav-link(data-category='applications') Applications
            .row.works-content-container